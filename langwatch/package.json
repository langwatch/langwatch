{
  "name": "langwatch",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build --no-lint",
    "lint": "next lint",
    "dev": "npm run start:prepare:files && npm run start:prepare:db && NODE_ENV=development DEBUG=langwatch:* DEBUG_HIDE_DATE=true DEBUG_COLORS=true concurrently 'node src/server.js' 'npm run start:workers'",
    "start": "next start",
    "start:prepare:files": "npm run copy:integration && npm run copy:langevals-types && npm run types:zod:generate",
    "start:prepare:db": "npm run prisma:db:push && npm run elastic:index:create",
    "start:workers": "tsx --tsconfig tsconfig.workers.json src/workers.ts",
    "test": "npm run test:unit -- --run --bail=1 && npm run test:integration -- --run --bail=1 --single-thread",
    "test:unit": "DEBUG=langwatch:* DEBUG_HIDE_DATE=true DEBUG_COLORS=true vitest",
    "test:integration": "DEBUG=langwatch:* DEBUG_HIDE_DATE=true DEBUG_COLORS=true vitest -c vitest.integration.config.ts",
    "task": "DEBUG=langwatch:* tsx --tsconfig tsconfig.workers.json src/task.ts",
    "prisma:db:push": "npx prisma db push",
    "elastic:index:create": "npm run task -- elasticIndexCreate",
    "types:zod:generate": "ts-to-zod src/server/tracer/types.ts src/server/tracer/types.generated.ts && ts-to-zod src/trace_checks/types.ts src/trace_checks/types.generated.ts && ts-to-zod src/trace_checks/evaluators.generated.ts src/trace_checks/evaluators.zod.generated.ts",
    "copy:integration": "cp -R ../docs/docs/integration-guides src/components/",
    "copy:langevals-types": "cd src/trace_checks && curl -L \"https://raw.githubusercontent.com/langwatch/langevals/main/ts-integration/evaluators.generated.ts?$(date +%s)\" -o evaluators.generated.ts"
  },
