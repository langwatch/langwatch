{
  "name": "langwatch",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "npm run start:prepare:files && npm run build:websocket && BUILD_TIME=1 next build --no-lint && rm -rf .next/cache",
    "build:websocket": "esbuild src/optimization_studio/server/socketServer.ts --bundle --platform=node --outfile=build-websocket/socketServer.js --packages=external --alias:@injected-dependencies.server=src/injection/injection.server.ts",
    "lint": "next lint",
    "dev": "npm run start:prepare:files && npm run build:websocket && NODE_ENV=development npm run start",
    "start": "./scripts/start.sh",
    "start:app": "tsx src/server.ts",
    "start:quickwit": "cd quickwit && ./quickwit run",
    "start:prepare:files": "npm run copy:langevals-types && npm run copy:llm-model-costs && npm run types:zod:generate && npm run prisma:generate:typescript",
    "start:prepare:db": "npm run prisma:migrate && npm run elastic:migrate",
    "start:workers": "tsx --tsconfig tsconfig.workers.json src/workers.ts",
    "test": "npm run test:unit -- --run --bail=1 && npm run test:integration -- --run --bail=1 --single-thread",
    "test:unit": "DEBUG=langwatch:* DEBUG_HIDE_DATE=true DEBUG_COLORS=true vitest",
    "test:integration": "DEBUG=langwatch:* DEBUG_HIDE_DATE=true DEBUG_COLORS=true vitest -c vitest.integration.config.ts",
    "test:stress": "DEBUG=langwatch:* DEBUG_HIDE_DATE=true DEBUG_COLORS=true vitest -c vitest.stress.config.ts",
    "task": "DEBUG=langwatch:* tsx --tsconfig tsconfig.workers.json src/task.ts",
    "prisma:generate:typescript": "npx prisma generate",
    "prisma:generate:migration": "npx prisma migrate dev --create-only",
    "prisma:migrate": "npx prisma migrate deploy",
    "elastic:migrate": "npm run task elasticMigrate",
    "types:zod:generate": "./scripts/generate-zod-types.sh",
    "copy:langevals-types": "cd src/server/evaluations && curl -L \"https://raw.githubusercontent.com/langwatch/langevals/main/ts-integration/evaluators.generated.ts?$(date +%s)\" -o evaluators.generated.ts",
    "copy:llm-model-costs": "cd src/server/modelProviders && curl -L \"https://raw.githubusercontent.com/BerriAI/litellm/main/model_prices_and_context_window.json\" -o llmModelCosts.json",
    "setup:quickwit": "./scripts/install-quickwit.sh",
    "tsc-watch": "tsc --noEmit --watch",
    "test:e2e": "playwright test --config=e2e/playwright.config.ts",
    "test:e2e:save-auth-state": "tsx e2e/save-auth-state.ts",
    "test:e2e:codegen": "playwright codegen --load-storage=./e2e/auth.json http://localhost:5560/"
  },
