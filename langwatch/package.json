{
  "name": "langwatch",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build --no-lint",
    "dev": "NODE_ENV=development DEBUG=langwatch:* DEBUG_HIDE_DATE=true DEBUG_COLORS=true concurrently 'node src/server.js' 'npm run start:workers'",
    "postinstall": "cp -R ../docs/docs/integration-guides src/components/ && prisma generate && prisma db push && npm run types:zod:generate",
    "lint": "next lint",
    "start": "concurrently 'node src/server.js' 'npm run start:workers'",
    "start:workers": "tsx src/workers.ts",
    "prisma:db:push": "npx prisma db push",
    "test": "DEBUG_HIDE_DATE=true DEBUG_COLORS=true vitest",
    "test:integration": "DEBUG_HIDE_DATE=true DEBUG_COLORS=true vitest -c vitest.integration.config.ts",
    "elastic:index:create": "tsx src/task.ts elasticIndexCreate",
    "types:zod:generate": "ts-to-zod src/server/tracer/types.ts src/server/tracer/types.generated.ts"
  },
  "dependencies": {
    "@chakra-ui/next-js": "^2.1.5",
    "@chakra-ui/react": "^2.8.1",
    "@elastic/elasticsearch": "^8.10.0",
