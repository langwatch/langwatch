# Traces Parity Check Configuration
# Copy this file to .env and fill in the values

# LangWatch API URL
BASE_URL=http://localhost:3000

# ClickHouse project (featureClickHouseDataSourceTraces = true)
CH_API_KEY=
CH_PROJECT_ID=

# Elasticsearch project (featureClickHouseDataSourceTraces = false)
ES_API_KEY=
ES_PROJECT_ID=

# Optional settings
TOLERANCE=0.05                    # Numeric tolerance (5%)
TRACE_COUNT=5                     # OTEL traces per variation (default 5)
WAIT_TIME_MS=120000               # Ingestion poll timeout
RUN_PYTHON_EXAMPLES=true          # Toggle Python SDK phase
RUN_SNIPPETS=true                 # Toggle Onboarding-sourced snippets
PYTHON_SDK_DIR=../../../python-sdk   # Path to python-sdk (relative to this package)
