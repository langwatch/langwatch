{
  "name": "langwatch",
  "version": "0.2.0",
  "description": "",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/langwatch/langwatch.git",
    "directory": "typescript-sdk"
  },
  "scripts": {
    "prepare": "./copy-types.sh",
    "test": "vitest",
    "prebuild": "npm run prepare",
    "build": "tsup && esbuild src/index.ts --bundle --platform=node --outfile=dist/index.js",
    "prepublish": "npm run build",
    "pull:openapispec": "curl https://raw.githubusercontent.com/langwatch/docs/refs/heads/main/api-reference/openapiLangWatch.json -o ./src/api-client/langwatch-openapi.json",
    "generate:openapi-types": "npm run pull:openapispec && npx openapi-typescript ./src/api-client/langwatch-openapi.json -o ./src/api-client/langwatch-openapi.ts"
  },
  "author": "",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@ai-sdk/openai": "^0.0.18",
    "@eslint/js": "^9.4.0",
    "@types/debug": "^4.1.12",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^16.0.0",
    "dotenv": "^16.4.5",
    "esbuild": "^0.21.5",
    "eslint": "^8.57.0",
    "ts-to-zod": "3.8.5",
    "tsup": "^8.1.0",
    "typescript": "^4.9.5",
    "typescript-eslint": "^7.11.0",
    "vitest": "^0.5.0"
  },
  "dependencies": {
    "@langchain/core": "^0.2.7",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/otlp-transformer": "^0.202.0",
    "@opentelemetry/sdk-trace-base": "^2.0.1",
    "ai": "^4.0.14",
    "eventemitter3": "^5.0.1",
    "fets": "^0.8.5",
    "javascript-stringify": "^2.1.0",
    "nanoid": "^5.0.9",
    "openai": "^4.47.3",
    "openapi-fetch": "^0.14.0",
    "zod": "^3.22.4",
    "zod-validation-error": "^3.3.0"
  }
}
